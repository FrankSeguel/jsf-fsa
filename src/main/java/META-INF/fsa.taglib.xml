<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
                version="2.0">
    
    <namespace>http://www.fsa.cl/jsf</namespace>
    
    
    <tag>
        <tag-name>messages</tag-name>
        <source>taglib/messages.xhtml</source>
    </tag>
    
    <tag>
        <tag-name>inputDate</tag-name>
        <source>taglib/input-date.xhtml</source>
        <attribute>
            <name>idcomponent</name>
            <description>Nombre del Componente</description>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>value</name>
            <description>Nombre del Componente</description>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>required</name>
            <description>Marca Requerido</description>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <name>readonly</name>
            <description>Marca readonly</description>
            <type>java.lang.Boolean</type>
        </attribute>
    </tag>
    
    <tag>
        <tag-name>inputDropdown</tag-name>
        <source>taglib/input-dropdown.xhtml</source>
        <attribute>
            <name>idcomponent</name>
            <description>Nombre del Componente</description>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>disabled</name>
            <description>Desactiva</description>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <name>required</name>
            <description>Marca Requerido</description>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <name>dropdownbean</name>
            <description>Drop Down Bean</description>
            <required>true</required>
        </attribute>
        <attribute>
            <name>filter</name>
            <description>Despliega filtro</description>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <name>filterMatchMode</name>
            <description>Match mode for filtering, valid values are startsWith (default), contains, endsWith and custom.</description>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>form</name>
            <description>id del formulario form.</description>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>submitwhenchange</name>
            <description>submitwhenchange </description>
            <type>java.lang.Boolean</type>
        </attribute>
    </tag>

    <tag>
        <tag-name>inputText</tag-name>
        <source>taglib/inputText.xhtml</source>
        <attribute>
            <name>idcomponent</name>
            <description>Nombre del Componente</description>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>value</name>
            <description>Nombre del Componente</description>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>required</name>
            <description>Marca Requerido</description>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <name>readonly</name>
            <description>Marca readonly</description>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <name>type</name>
            <description>type : text, email, password</description>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>placeholder</name>
            <description>placeholder</description>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>title</name>
            <description>title</description>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>requiredMessage</name>
            <description>requiredMessage</description>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>validatorMessage</name>
            <description>validatorMessage</description>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>size</name>
            <description>size</description>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>maxlength</name>
            <description>maxlength</description>
            <type>java.lang.String</type>
        </attribute>
    </tag>

    <tag>
        <tag-name>button</tag-name>
        <source>taglib/common-button.xhtml</source>
        <attribute>
            <name>idcomponent</name>
            <description>Nombre del Componente</description>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>bean</name>
            <description>Bean</description>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>metodo</name>
            <description>metodo</description>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>value</name>
            <description>Nombre del Componente</description>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    
</facelet-taglib>